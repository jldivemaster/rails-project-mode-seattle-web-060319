<h1>Users#new</h1>
<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :age %>
<%= f.text_field :age %>
<%= f.label :bio %>
<%= f.text_area :bio %>
<%= f.label :skills %>
<%= f.text_field :skills %>
<%= f.submit %>XXX
<% end %>

<br>
<br>
<%= render '/layouts/home_link' %>
