<div class="content">
  <div class="maincontent">
    <h1>Log In:</h1>
    <%= form_for :login do |f| %>
      <%= f.label :username %>
      <%= f.text_field :username %><br><br>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <br><br>
      <%= f.submit "Log In", class: "button" %>
    <% end %>
  </div>
</div>
